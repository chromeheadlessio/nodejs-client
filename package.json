{
    "name": "nodejs-client",
    "homepage": "https://chromeheadless.io",
    "version": "1.0.0",
    "license": "MIT",
    "description": "nodejs client for chromeheadless.io service",
    "scripts": {
        "start": "nodemon --exec babel-node src/server.js",
        "watch": "babel -w src/ -d dist/",
        "build": "babel src -d dist/",
        "serve": "node dist/server.js",
        "test": "nodemon --exec babel-node src/test.js"
    },
    "dependencies": {
        "request": "^2.88.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "@babel/runtime": "^7.2.0",
        "nodemon": "^1.18.9"
    }
}
